# GitHub OAuth credentials (create app at https://github.com/settings/developers)
GITHUB_CLIENT_ID=your_client_id
GITHUB_CLIENT_SECRET=your_client_secret

# JWT secret for signing tokens (generate a random string)
JWT_SECRET=your_jwt_secret_key_change_me

# Comma-separated list of allowed GitHub usernames
ALLOWED_USERS=user1,user2

# Frontend URL (for CORS and OAuth redirects)
FRONTEND_URL=http://localhost:5173

# Path to tab-data directory (relative to backend/)
TAB_DATA_PATH=../tab-data
